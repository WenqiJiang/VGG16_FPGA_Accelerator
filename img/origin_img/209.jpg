		$('.tdns ul.submenu').hide();
		$(this).find('> ul').show();
		body.removeClass('hide-menu-buffer');
	}).on('mouseleave', function(e){
		$('.tdns ul.submenu').hide();
	});	

	$('ul.submenu').on('mousemove', function(e){
		body.removeClass('hide-menu-buffer');
		removeBuffer();
	});	

	if (/iphone|ipad|android/i.test(navigator.userAgent)){
		
		$('li.tdns > a.ddme').on('click', function(e){		
			$('.tdns ul.submenu').hide();
			$(this).next('ul').show();
			return false;
		});

		$('body').on('click', function(e){
			$('.tdns ul.submenu').hide();
		});

		document.getElementById('desktop-menu-ux-fix').innerHTML = '';
	}

})(jQuery);

</script>



