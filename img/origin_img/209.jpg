	<script type="text/javascript">
var uReadStat;
var omo_currentuser = null;
var omo_userentitled = false;
var uReadCd = 1;

$(function(){	
	if(confirmSubscriptionOn()){			
		ureadCheckCookie();
		ureadUpdateMeterBar();
	}
});

function ureadSetDefault(){
	var uread_omo_sat = getCookie('OMO_METERLMT') || null;
	if(uread_omo_sat == null){
		setCookie('uread_stat','{"maxart":5,"maxvdo":5,"maxgen":5,"recent_read_art":[],"recent_read_vdo":[]}',1);
		uReadStat = {"maxart":5,"maxvdo":5,"maxgen":5,"recent_read_art":[],"recent_read_vdo":[]};
	} else {
		uread_omo_sat = JSON.parse(uread_omo_sat);
		var vdolmt = artlmt = genlmt = 0;
		uread_omo_sat.forEach(function(elem){
		switch(elem.type){
			case "article":
			artlmt = elem.count;
			break;
			case "video":
			vdolmt = elem.count;
			break;
			case "general":
			genlmt = elem.count;
			uReadCd = Number(Number(elem.cooldown.substring(0,2))/24).toFixed(1);
			break;
		}

		});
		uReadStat = {"maxart":artlmt,"maxvdo":vdolmt,"maxgen":genlmt,"recent_read_art":[],"recent_read_vdo":[]};
		setUreadCookie('uread_stat',JSON.stringify(uReadStat));
	}
}

function ureadCheckCookie(){
	uReadStat = getCookie('uread_stat') || null;
	if(uReadStat == null){
		ureadSetDefault();
	} else{
		uReadStat = JSON.parse(uReadStat);
	}

	//refresh omo user data 
	omo_currentuser = OMOSDK.auth().getUserInfo() || null;
	omo_userentitled = atob(getCookie('OMO_ISENTITLED')) || null;
	if(omo_userentitled == null){
		omo_userentitled = false;		
	} else {
		omo_userentitled = (omo_userentitled=='true');
	}
}

function ureadArticle(objid,objtype){
	if(confirmSubscriptionOn()){
		ureadCheckCookie();	
		if(!omo_currentuser.isLoggedIn || !omo_userentitled){
		if(objtype=='VIDEO'){
			if(uReadStat.recent_read_vdo.indexOf(objid) == "-1"){		
			uReadStat.recent_read_vdo.push(objid);
			setUreadCookie('uread_stat',JSON.stringify(uReadStat));
			}
		} else{
			if(uReadStat.recent_read_art.indexOf(objid) == "-1"){		
			uReadStat.recent_read_art.push(objid);
			setUreadCookie('uread_stat',JSON.stringify(uReadStat));
			}
		}		
		}
		ureadUpdateMeterBar();
	}
}

function ureadMeterReached(objid,checktype){
	if(confirmSubscriptionOn()){
		ureadCheckCookie();
		if(!omo_currentuser.isLoggedIn || !omo_userentitled){
			if(checktype=="VIDEO" && uReadStat.recent_read_vdo.indexOf(objid) != "-1")
			return false;
			if(checktype=="ARTICLE" && uReadStat.recent_read_art.indexOf(objid) != "-1")
			return false;

		var metermax = uReadStat.maxgen ;
		var metercnt = uReadStat.recent_read_vdo.length+uReadStat.recent_read_art.length;
		if(metercnt >= metermax){
			return true;
		} else{
			return false;
		}

		} else{
			return false;
		}
	} else {
		return false;
	}
}

function ureadMeterRemain(checktype){
	if(confirmSubscriptionOn()){
		ureadCheckCookie();
		if(!omo_currentuser.isLoggedIn || !omo_userentitled){
		var metermax = uReadStat.maxgen ;
		var metercnt = uReadStat.recent_read_vdo.length+uReadStat.recent_read_art.length ;
		if(metercnt >= metermax){
			return 0;
		} else{
			return metermax - metercnt;
		}
		} else{
			return 'N/A';
		}
	} else {
		return 'N/A';
	}
}

function uReadDisplayMsgBox(showdef){
	setTimeout(function() {
		if(confirmSubscriptionOn()){					
			ureadCheckCookie();	
			if(!omo_currentuser.isLoggedIn || !omo_userentitled){
			$('div.content').addClass('block');
			$('div.contentblock-msg div.description').text('會員專區 只限會員閱讀');
			var vdocontainer = $('#video_player iframe');
			if(typeof vdocontainer !=="undefined"){
				vdocontainer.remove();
			}
			if(showdef){
				$('#video_player').html('<image src="https://staticlayout.appledaily.hk/web_images/layout/cntb_vid_bg.png">');				
			} else{
				$('#video_player').html('<image src="https://static.appledaily.hk/images/apple-photos/video/20190420/720pix/1555698291_dec1.jpg" width="520px">');
			}
			$("div.ArticleContent_Outer:not(:first)").remove();
			$("div.photo_center a.fancybox-button").remove();
			
			$('div.contentblock-block').css('display','block');						
			}
		};
	}, 2000);
}

function ureadUpdateMeterBar(){
	if(confirmSubscriptionOn()){
		if(!omo_currentuser.isLoggedIn || !omo_userentitled){
			var artcnt = ureadMeterRemain('ARTICLE');
			var msg = "今日你還可閱讀<span>"+artcnt+"</span>篇內容";
			$("#uread_meterbar").html(msg);
		} else if (omo_userentitled){
			$("#uread_meterbar").html('');
		}
	}
}
function ureadOMOLogin(){
	var omo_params = ureadPrepareParams();
	if(omo_params != null)	
	OMOSDK.logger().extraUTMParams(omo_params);

	OMOSDK.auth().redirectLogin();
}

function ureadPrepareParams(){
	var resu = {
		utm_source: '', 
		utm_medium: '', 
		utm_campaign: '', 
		utm_term: '', 
		utm_content: ''
	};

	var urlparams = window.location.search.substring(1);
	urlparams = urlparams.split('&');
	if(urlparams.length==1){
		return null;
	}
	for (var i = 0; i < urlparams.length; i++)
	{
		var sParameterName = urlparams[i].split('=');
		switch(sParameterName[0]){
			case 'utm_source':
				resu['utm_source'] = sParameterName[1];
			break;
			case 'itm_source':
				resu['utm_source'] = sParameterName[1];
			break;

			case 'utm_medium':
				resu['utm_medium'] = sParameterName[1];
			break;
			case 'itm_medium':
				resu['utm_medium'] = sParameterName[1];
			break;

			case 'utm_campaign':
				resu['utm_campaign'] = sParameterName[1];
			break;
			case 'itm_campaign':
				resu['utm_campaign'] = sParameterName[1];
			break;

			case 'utm_content':
				resu['utm_content'] = sParameterName[1];
			break;
			case 'itm_content':
				resu['utm_content'] = sParameterName[1];
			break;

			case 'utm_term':
				resu['utm_term'] = sParameterName[1];
			break;
			case 'itm_term':
				resu['utm_term'] = sParameterName[1];
			break;
		};
	}
	if(resu['utm_source'] == ''){
		return null;
	}
	return resu;
}

</script>

